{
  "name": "tokendistributionv1",
  "version": "1.0.0",
  "description": "TokenDistributionV1",
  "main": "index.js",
  "scripts": {
    "generate-wallet": "npx hardhat run scripts/generate-wallet.ts",
    "deploy": "bash ./scripts/npm-deploy.sh",
    "test:echidna": "solc-select use 0.8.25 && echidna --contract EchidnaTokenDistribution echidna/EchidnaTokenDistribution.sol --config echidna/echidna.config.yaml",
    "test:hardhat": "npx hardhat test",
    "docker:clear": "sudo docker compose rm -f && sudo docker system prune",
    "docker:clear-all": "sudo docker compose rm -f && sudo docker system prune -a",
    "docker:stop-all": "sudo docker rm -vf $(docker ps -aq)",
    "docker:instance": "bash ./scripts/docker/launch-instance.sh",
    "docker:test:echidna": "bash ./scripts/docker/test-echidna.sh",
    "docker:test:hardhat": "bash ./scripts/docker/test-hardhat.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngi-indi/module-blockchain.git#experimental"
  },
  "keywords": [
    "erc20",
    "solidity",
    "hardhat"
  ],
  "author": "strausste",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngi-indi/module-blockchain/issues"
  },
  "homepage": "https://github.com/ngi-indi/module-blockchain/tree/experimental#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "chai": "^4.1.2",
    "hardhat": "^2.22.5",
    "mocha": "^10.4.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2"
  }
}
